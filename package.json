{
  "name": "private-asset-registry",
  "version": "0.1.0",
  "description": "Private Asset Registry & Transfer Rules Engine MVP",
  "main": "src/backend/server.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:fresh": "npm run clean:frontend && npm run dev",
    "predev:backend": "node scripts/free-port.js 3001",
    "dev:backend": "nodemon --watch src/backend --exec tsx src/backend/server.ts",
    "predev:frontend": "npm run clean:frontend",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:frontend:fresh": "npm run clean:frontend && npm run dev:frontend",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc --project tsconfig.backend.json",
    "build:frontend": "cd src/frontend && npm run build",
    "test": "vitest",
    "test:all": "powershell -ExecutionPolicy Bypass -File ./scripts/run-tests.ps1",
    "test:ui": "vitest --ui",
    "migrate": "tsx scripts/migrate.ts",
    "normalize:admin": "tsx scripts/normalize-admin-user.ts",
    "seed": "npm run normalize:admin && tsx scripts/seed-demo.ts",
    "seed:basic": "npm run normalize:admin && tsx scripts/seed-data.ts",
    "seed:demo": "npm run normalize:admin && tsx scripts/seed-demo.ts",
    "seed:showcase": "npm run normalize:admin && tsx scripts/seed-showcase.ts",
    "clean:frontend": "node scripts/clean-next-cache.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "mcp": "tsx src/mcp/server.ts"
  },
  "keywords": [
    "private-assets",
    "compliance",
    "transfer-rules",
    "mvp"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20 <21"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.18.0",
    "sql.js": "^1.10.3",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/pdfkit": "^0.17.4",
    "@types/pg": "^8.16.0",
    "@types/sql.js": "^1.4.9",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
